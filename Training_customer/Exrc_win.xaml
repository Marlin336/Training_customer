<Window x:Class="Training_customer.Exrc_win"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Training_customer"
        mc:Ignorable="d"
        Title="Exrc_win" Height="450" Width="325">
    <Grid>
        <GroupBox x:Name="gb_muscle" Margin="10,70,10,10">
            <GroupBox.Header>
                <Label Content="Мышцы" FontSize="16" FontWeight="Bold"/>
            </GroupBox.Header>
            <TreeView x:Name="tv_main" FontSize="14">

            </TreeView>
        </GroupBox>
        <GroupBox Height="65" Margin="10,0" VerticalAlignment="Top">
            <GroupBox.Header>
                <Label Content="Название" FontSize="16" FontWeight="Bold"/>
            </GroupBox.Header>
            <TextBox x:Name="tb_name" IsReadOnly="True" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Margin="0"/>
        </GroupBox>
    </Grid>
</Window>
