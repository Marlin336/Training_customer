<Window x:Class="Training_customer.Passreq_win"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Training_customer"
        mc:Ignorable="d"
        Title="Подтверждение" Width="274" Height="152" ResizeMode="NoResize">
    <Grid>
        <Label Content="Введите пароль" Margin="10,10,10,0" VerticalAlignment="Top" HorizontalContentAlignment="Center"/>
        <PasswordBox x:Name="tb_pass" Margin="30,41,30,0" VerticalAlignment="Top"/>
        <Button x:Name="b_ok" Content="Подтвердить" Margin="90,75,86,0" Height="32" VerticalAlignment="Top" Click="Button_Click" IsDefault="True"/>

    </Grid>
</Window>
